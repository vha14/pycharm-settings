<keymap version="1" name="Default for GNOME copy" parent="Default for GNOME">
  <action id="ActivateFindToolWindow" />
  <action id="ActivatePythonConsoleToolWindow">
    <keyboard-shortcut first-keystroke="alt 4" />
  </action>
  <action id="ActivateRunToolWindow" />
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="shift alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="FileStructurePopup">
    <keyboard-shortcut first-keystroke="ctrl f12" />
    <keyboard-shortcut first-keystroke="alt back_slash" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="shift alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="shift alt minus" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <keyboard-shortcut first-keystroke="alt g" />
  </action>
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="ctrl alt s" />
    <keyboard-shortcut first-keystroke="alt comma" />
  </action>
</keymap>